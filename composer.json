{
    "name": "webong/cashier-paystack",
    "description": "A Laravel Cashier Wrapper that provides an expressive, fluent interface to Paystack's subscription invoicing services.",
    "keywords": ["laravel", "php", "paystack", "cashier", "payment", "billing", "subscription"],
    "license": "MIT",
    "authors": [
        {
            "name": "Wisdom A. Ebong",
            "email": "wisdomaebong@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/webong/lpaystack"
        },
        {
            "type": "vcs",
            "url": "https://github.com/webong/paystack"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "dompdf/dompdf": "^0.8.0",
        "illuminate/database": "^6.0|^7.0",
        "illuminate/support": "~5.8.0|^6.0|^7.0",
        "nesbot/carbon": "^2.0",
        "xeviant/laravel-paystack": "dev-master"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "^3.8|^4.0|^5.0",
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Webong\\Cashier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webong\\Cashier\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webong\\Cashier\\CashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
